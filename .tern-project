{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "underscore"
  ],
  "loadEagerly": [
    "app/**/*.js"
  ],
  "plugins": {
    "node-express": {},
    "complete_strings": {
      "maxLength": 15
    },
    "doc_comment": {
      "fullDocs": true,
      "strong": false
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "es_modules": {},
    "angular": {},
    "requirejs": {
      "baseURL": "",
      "paths": "",
      "override": ""
    }
  }
}